<h1>Votes#index</h1>
<%= form_tag "/", method: "post" do %>
  <input type="text" name="vote" />
  <input type="submit" value="Proposer un vote" />
<% end %>
<% @votes.each do |vote| %>
  <p><a href="/votes/<%= vote.id %>"><%= vote.id %> <%= vote.quest %> <%= vote.for %> <%= vote.against %></a></p>
<% end %>
